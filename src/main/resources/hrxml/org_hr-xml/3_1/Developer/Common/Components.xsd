			<xsd:extension base="RaterBaseType">
				<xsd:sequence>
					<xsd:element ref="OtherRating" minOccurs="0" maxOccurs="unbounded"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:element name="Rater" type="RaterType"/>
	<xsd:complexType name="RaterType">
